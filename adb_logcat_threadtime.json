{
  "android_logcat_time": {
    "title": "Android logcat with threadtime",
    "description": "logcat generated by command 'adb logcat -v threadtime'",
    "url": "https://developer.android.com/studio/command-line/logcat.html",
    "timestamp-format": [
      "%Y-%m-%d %H:%M:%S.%L"
    ],
    "regex": {
      "std-logcat-time": {
        "pattern": "^(?<timestamp>\\d{4}-\\d{2}-\\d{2}\\s*\\d{2}:\\d{2}:\\d{2}\\.\\d{3})\\s*(?<pid>\\d*)\\s*(?<tid>\\d*)\\s*(?<log_status>.{1})\\s*(?<tag>.*?\\s*):(?<body>.*)"
      }
    },
    "level-field": "log_status",
    "level": {
      "error": "E",
      "warning": "W",
      "info": "I",
      "debug": "D",
      "trace": "V"
    },
    "value": {
      "log_status": {
        "kind": "string",
        "foreign-key": true
      },
      "tag" : {"kind":"string", "identifier": true},
      "pid" : {"kind":"integer", "identifier":true},
      "tid" : {"kind":"integer", "identifier":true},
      "body": {"kind":"string", "identifier": false}
    },
    "sample": [
      {
	"line": "2019-08-13 05:18:07.535  1234  1235 I Android_Class_Tag: Android Logcat Message"
      }
    ]
  }
}
